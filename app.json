{
  "expo": {
    "name": "CTICU Schedule",
    "slug": "cticu-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cticumobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.zambrano.cticuschedule",
      "buildNumber": "2",
      "supportsTablet": false,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSExceptionDomains": {
            "cticu.zambrano.nyc": {
              "NSExceptionAllowsInsecureHTTPLoads": false
            }
          }
        }
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zambrano.cticuschedule",
      "versionCode": 1,
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cb88b812-ce54-4e5c-9931-bfb75b03ada5"
      }
    },
    "owner": "gzambran"
  }
}
